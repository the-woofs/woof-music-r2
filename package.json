{
  "name": "woof-music-r2",
  "version": "0.1.0",
  "private": true,
  "description": "Music player using YouTube Music (without area restrictions in most cases) and Youtube.",
  "license": "MIT",
  "main": "public/electron.js",
  "homepage": "./",
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.8.4",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^13.5.0",
    "antd": "4.20.6",
    "concurrently": "^7.2.2",
    "electron-forge": "^5.2.4",
    "electron-is-dev": "^2.0.0",
    "emotion": "^11.0.0",
    "firebase": "^9.8.2",
    "path": "^0.12.7",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-firebase-hooks": "^5.0.3",
    "react-player": "^2.10.1",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "state-pool": "^0.7.1",
    "wait-on": "^6.0.1",
    "web-vitals": "^2.1.4",
    "yt-channel-info": "^3.0.4"
  },
  "scripts": {
    "electron-dev": "concurrently \"BROWSER=none npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-package": "./node_modules/.bin/electron-builder -c.extraMetadata.main=build/start-electron.js",
    "preelectron-package": "yarn run build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "concurrently -k \"BROWSER=none yarn start\" \"yarn:electron\"",
    "electron": "wait-on tcp:3000 && electron .",
    "package": "react-scripts build && electron-forge package",
    "make-mac": "react-scripts build && electron-forge make --platform darwin",
    "make-linux": "react-scripts build && electron-forge make --platform linux",
    "make": "react-scripts build && electron-forge make"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@electron-forge/publisher-github": "^6.0.0-beta.64",
    "concurrently": "^7.2.2",
    "electron-prebuilt-compile": "8.2.0",
    "wait-on": "^6.0.1"
  },
  "config": {
    "forge": {
      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "repository": {
              "owner": "acutewoof",
              "name": "woof-music"
            }
          }
        }
      ]
    }
  }
}
